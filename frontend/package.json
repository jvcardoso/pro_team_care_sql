{
  "name": "pro-team-care-frontend",
  "version": "1.0.0",
  "description": "Pro Team Care - Frontend Application",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "serve -s dist",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:report": "playwright show-report playwright-report",
    "test:e2e:companies": "playwright test companies-crud-flow.spec.ts",
    "test:api": "newman run ../postman/Pro_Team_Care_API.postman_collection.json -e ../postman/Pro_Team_Care.postman_environment.json",
    "test:api:report": "newman run ../postman/Pro_Team_Care_API.postman_collection.json -e ../postman/Pro_Team_Care.postman_environment.json -r htmlextra --reporter-htmlextra-export newman-report.html",
    "test:all": "npm run test && npm run test:e2e && npm run test:api",
    "test:coverage": "jest --coverage && playwright test --reporter=html",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.3.0",
    "axios-retry": "^4.5.0",
    "chart.js": "^4.5.0",
    "clsx": "^1.2.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.260.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.8.0",
    "react-select": "^5.10.2",
    "recharts": "^3.2.1",
    "tailwindcss": "^3.2.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^30.1.2",
    "eslint": "^8.34.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^30.1.2",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "playwright": "^1.55.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "serve": "^14.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  },
  "keywords": [
    "react",
    "fastapi",
    "pro-team-care",
    "healthcare"
  ],
  "author": "Pro Team Care",
  "license": "ISC"
}
